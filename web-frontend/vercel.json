{
  "version": 2,
  "buildCommand": "cd web-frontend && npm run build",
  "outputDirectory": "web-frontend/.next",
  "installCommand": "npm install --force",
  "framework": "nextjs",
  "functions": {
    "web-frontend/src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["bom1"],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./web-frontend/"
}
