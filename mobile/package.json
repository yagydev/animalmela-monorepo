    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.87.1",
    "axios": "^1.6.0",
    "expo": "~49.0.0",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "~2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
