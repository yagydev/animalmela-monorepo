{
  "name": "pashu-marketplace-mobile",
  "version": "1.0.0",
  "description": "Pashu Marketplace Mobile App - React Native Application",
  "main": "index.js",
  "scripts": {
    "start": "expo start --port=8082",
    "start:metro-8082": "npx react-native start --port=8082",
    "android": "expo start --android --port=8082",
    "android:dev": "adb reverse tcp:8082 tcp:8082 && npx react-native run-android --port=8082",
    "ios": "expo start --ios --port=8082",
    "ios:dev": "npx react-native run-ios",
    "web": "expo start --web --port=8082",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c",
    "reset-cache": "expo start --clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
